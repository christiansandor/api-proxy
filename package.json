{
  "name": "@holdltd/api-proxy",
  "version": "0.9.0",
  "description": "API Proxy",
  "main": "dist/api-proxy.js",
  "author": "Sándor Krisztián <contact@christiansandor.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/christiansandor/api-proxy"
  },
  "keywords": [
    "proxy"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/api-proxy.js",
    "watch": "npm start -- -w",
    "start:dev": "nodemon watch:serve",
    "watch:dev": "nodemon watch:serve -- -w"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/api-proxy.ts"
  },
  "bin": {
    "api-proxy": "./dist/api-proxy.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/request": "^2.48.5",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^16.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rxjs": "^6.6.6",
    "yaml": "^1.10.0",
    "yargs": "^16.2.0"
  }
}
